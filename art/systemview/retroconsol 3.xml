<?xml version="1.0" encoding="UTF-8"?>
<theme>
<formatVersion>7</formatVersion>
<view name="system">
<image name="background" extra="true">
<tile>false</tile>
<size>1 1</size>
<pos>0 0</pos>
<origin>0 0</origin>
<path>./art/fondos/system 3.png</path>
<zIndex>30</zIndex>
</image>
  
<image name="background3" extra="true">
<tile>false</tile>
<size>1 1</size>
<pos>0 0</pos>
<origin>0 0</origin>
<path>./art/sistemas/wall/default.png</path>
<path>./art/sistemas/wall/${system.theme}.png</path>
<zIndex>10</zIndex>
</image>
   
<image name="predeterminado" extra="true">
<tile>false</tile>
<Delay>800</Delay>
<pos>0.7425 0.610</pos>
<size>0.2278 0.4110</size>
<origin>0.5 0.5</origin>
<path>./art/fondos/default.png</path>
<zIndex>11</zIndex>
<storyboard>
        <animation property="y" from="0.05" begin="0" duration="350" mode="easeOut"/>
        <animation property="opacity" from="0" to="1" duration="2950" mode="easeIn"/>
        <animation property="scale" from="0" to="1" duration="350" mode="easeOut"/>
      </storyboard>
</image>

 
    <video name="backgroundvideo" extra="true">
<visible>true</visible>
<path>{random}</path>                  
<pos>0.7425 0.610</pos>
<size>0.2278 0.4110</size>
<origin>0.5 0.5</origin>
<delay>0</delay>
<showSnapshotNoVideo>false</showSnapshotNoVideo>
<showSnapshotDelay>false</showSnapshotDelay>
<color>FFFFFF</color>   
<zIndex>15</zIndex>
<storyboard>  
        <animation property="y" from="0" begin="0" duration="800" mode="easeOut"/>  
</storyboard>
</video>

<video name="backgroundvideo3" extra="true">
<visible>true</visible>
<path>./art/videos/Snakervill.mp4</path>               
<pos>0.745  0.309</pos>
<size>0.260 0.095</size>
<origin>0.5 0.5</origin>
<delay>0</delay>
<showSnapshotNoVideo>false</showSnapshotNoVideo>
<showSnapshotDelay>false</showSnapshotDelay>
<color>FFFFFF</color>   
<zIndex>10</zIndex>
</video>

<video name="backgroundvideo" extra="true" ifHelpPrompts="false">
<pos>0.5 0.5</pos>
<minSize>1 1</minSize>
</video>
<image name="ControllerOverlay" extra="true">
<visible>false</visible>
</image>
<image name="CenterFade" extra="true">
<color>000000A0</color>
</image>    

<carousel name="systemcarousel">
<zIndex>43</zIndex>	      
<type>vertical</type>
<pos>-0.357 0.08</pos>
<size>1 0.840</size>
<logoScale>1.45</logoScale>
<logoSize>0.150 0.260</logoSize>
<color>00000000</color>
<maxLogoCount>05</maxLogoCount>
<logoRotation>15</logoRotation>
<logoRotationOrigin>3.0 0.5</logoRotationOrigin>
<logoAlignment>center</logoAlignment>   
<zIndex>90</zIndex>  
</carousel>

<text name="systemInfo">
<systemInfoDelay>300</systemInfoDelay>
<pos>0.110  0.7600</pos>
<size>0.420 0.35</size>
<fontSize>0.06</fontSize>
<fontPath>./art/fuentes/Anticlimax.ttf</fontPath>
<backgroundColor>00000000</backgroundColor>
<color>ffffff</color>
<glowColor>000000</glowColor>
<glowSize>0</glowSize>
<reflexion>0.0 0.0</reflexion>
</text>
        
<helpsystem name="help">
<visible>false</visible>
<textColor>1C00ff00</textColor>
<iconColor>1C00ff00</iconColor>
<fontSize>0.020</fontSize>
<pos>0.50 0.03</pos>
</helpsystem>
    
<text name="logoText">
<fontPath>./art/fuentes/Bebas Neue.ttf</fontPath>
<color>${baseColor}</color>
<forceUppercase>true</forceUppercase>
</text>
<image name="logo">
<path>./art/sistemas/silverring/default.png</path>
<path>./art/sistemas/silverring/${system.theme}.png</path>
<path>./${system.theme}/art/system-${lang}.png</path>
<path>./${system.theme}/art/system-${lang}.svg</path>
</image>
</view>
</theme>
